{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist --format=esm --packages=external",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.29",
    "@ai-sdk/google": "^3.0.13",
    "@ai-sdk/openai": "^3.0.18",
    "@google/genai": "^1.38.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.91.0",
    "ai": "^6.0.58",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "openai": "^6.16.0",
    "pdf-parse": "^2.4.5",
    "replicate": "^1.4.0",
    "sharp": "^0.33.5",
    "turndown": "^7.2.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.0",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^2.1.9",
    "esbuild": "^0.21.5",
    "supabase": "^2.72.8",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
